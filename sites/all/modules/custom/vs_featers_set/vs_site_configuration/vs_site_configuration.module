<?php

function vs_product_get_num_ending($number, $endingArray = array('', 'Ğ°', 'Ğ¾Ğ²')) {
  $number = $number % 100;
  if ($number>=11 && $number<=19) {
    $ending=$endingArray[2];
  }
  else {
    $str = (string)$number;
    $len = strlen($str);
    $i = $str[$len-1];
    switch ($i)
    {
      case (1): $ending = $endingArray[0]; break;
      case (2):
      case (3):
      case (4): $ending = $endingArray[1]; break;
      default: $ending=$endingArray[2];
    }
  }
  return $ending;
}